# Upstream host (do not include trailing slash)
TOPM_BASE_URL=https://topm.tech

# Upstream request timeouts (seconds)
TOPM_CONNECT_TIMEOUT=5
TOPM_READ_TIMEOUT=60
# Whether to use system proxy env vars (HTTP(S)_PROXY/ALL_PROXY)
TOPM_TRUST_ENV=1

# Comma-separated list of allowed frontend origins
CORS_ORIGINS=*

# Listen port
PORT=5051
